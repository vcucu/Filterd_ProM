<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="940.0" spacing="5.0" stylesheets="@../css/ComputationCell.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Circle fill="#787878" radius="8.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
         <styleClass>
            <String fx:value="circle-button" />
            <String fx:value="hover-show" />
            <String fx:value="button-add" />
         </styleClass></Circle>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <Region maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="14.0" style="-fx-background-color: green;" HBox.hgrow="ALWAYS" />
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="ALWAYS">
               <children>
                  <HBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" prefHeight="35.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <ComboBox prefHeight="20.0" prefWidth="150.0" styleClass="dropdown" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" minHeight="24.0" minWidth="24.0" text="">
                        <styleClass>
                              <String fx:value="circle-button" />
                              <String fx:value="button-play" />
                         </styleClass></Button>
                        <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <TextField alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" styleClass="cell-name" text="Cell name" />
                        <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <ComboBox prefHeight="20.0" prefWidth="150.0" styleClass="dropdown" HBox.hgrow="ALWAYS" />
                        <MenuButton mnemonicParsing="false">
                          <items>
                            <MenuItem mnemonicParsing="false" onAction="#removeCell" text="Remove Cell" />
                          </items>
                        </MenuButton>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="24.0" layoutX="180.0" layoutY="15.0" stroke="BLACK" strokeType="INSIDE" styleClass="rectangle-button" width="24.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                     <children>
                        <ScrollPane fitToWidth="true" hbarPolicy="NEVER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="390.0" prefWidth="245.0" HBox.hgrow="ALWAYS">
                           <content>
                              <VBox fx:id="filtersVBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
                                 <children>
                                    <VBox fx:id="panelLayout" alignment="TOP_CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                                       <VBox.margin>
                                          <Insets top="5.0" />
                                       </VBox.margin>
                                    </VBox>
                                    <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#addFilter" styleClass="addFilterButton" text="Click to add a filter" VBox.vgrow="ALWAYS">
                                       <VBox.margin>
                                          <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
                                       </VBox.margin>
                                    </Label>
                                 </children>
                              </VBox>
                           </content>
                        </ScrollPane>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="46.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="11.0">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Rectangle>
                        <Pane fx:id="visualizerPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="390.0" minWidth="600.0" HBox.hgrow="ALWAYS" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="51.0">
         <children>
            <Button fx:id="autoButton" mnemonicParsing="false" onAction="#setComputationModeToAutomatic" text="Auto">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="manualButton" mnemonicParsing="false" onAction="#setComputationModeToManual" text="Manual">
               <HBox.margin>
                  <Insets left="3.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="computeButton" mnemonicParsing="false" text="▶">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <Button fx:id="exportButton" mnemonicParsing="false" text="▶ Export to workspace">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ScrollPane fx:id="scrollPane" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <content>
            <VBox alignment="CENTER">
               <children>
                  <!-- <Pane fx:id="configurationModal" prefHeight="200.0" prefWidth="200.0" /> -->
                  <VBox fx:id="layout" alignment="TOP_CENTER" spacing="5.0" VBox.vgrow="ALWAYS" />
                  <HBox alignment="CENTER" prefHeight="85.0" prefWidth="400.0" visible="false">
                     <children>
                        <Button fx:id="addComputationCellButton" mnemonicParsing="false" text="Computation Cell" textAlignment="CENTER">
                           <HBox.margin>
                              <Insets right="50.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="addTextCellButton" mnemonicParsing="false" text="Text Cell" textAlignment="CENTER" />
                     </children>
                  </HBox>
                  <Button fx:id="addCellButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addCell" text="+" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets bottom="20.0" top="30.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </content>
         <opaqueInsets>
            <Insets />
         </opaqueInsets></ScrollPane>
   </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxWidth="1.7976931348623157E308" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="title" alignment="CENTER" text="Filter BLA BLA Configuration" VBox.vgrow="ALWAYS">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <HBox fx:id="mainPanel" spacing="10.0">
         <children>
            <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
            <VBox fx:id="rightPanel">
               <children>
                  <VBox fx:id="slidersPanel" spacing="5.0" />
                  <VBox fx:id="othersPanel" spacing="5.0" />
                  <VBox fx:id="maybesPanel" spacing="5.0" />
               </children>
            </VBox>
            <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
